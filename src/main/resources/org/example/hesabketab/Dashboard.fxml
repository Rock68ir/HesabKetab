<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="DashboardBorderpane" prefHeight="720.0" prefWidth="1120.0" style="-fx-background-color: #b3b4bd;" stylesheets="@sidebarstyle.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.hesabketab.DashboardController">
   <top>
      <ToolBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
            <Button fx:id="hamburgerButton" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#OnBurgerClicked" prefHeight="15.0" prefWidth="59.0">
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/icons/Burgermenu.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label alignment="CENTER" text="Welcome">
               <font>
                  <Font name="Arial Black" size="14.0" />
               </font>
            </Label>
            <Pane fx:id="DashboardTime" nodeOrientation="RIGHT_TO_LEFT" prefHeight="34.0" prefWidth="452.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="timeLabel" alignment="TOP_RIGHT" contentDisplay="CENTER" text="Label" textAlignment="CENTER">
                     <font>
                        <Font name="Arial Italic" size="16.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </Pane>
        </items>
      </ToolBar>
   </top>
</BorderPane>
